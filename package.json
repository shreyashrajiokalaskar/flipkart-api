{
  "name": "flipkart-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "nodemon app.ts",
    "build": "tsc && tsc-alias",
    "start:dev": "ts-node-dev -r tsconfig-paths/register ./libs/app.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "cd ./libs/migrations && npm run typeorm -- migration:create ./libs/migrations/$npm_config_name",
    "typeorm:up": "npm run typeorm  -- -d ./libs/configs/data-source.ts  migration:run",
    "typeorm:generate-migration": "npm run typeorm migration:generate  -- -d ./libs/configs/data-source.ts ./libs/migrations/$npm_config_name",
    "typeorm:down": "npm run typeorm  -- -d ./libs/configs/data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm migration:show",
    "seed:run": "ts-node -r tsconfig-paths/register libs/seeders/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.16",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "socket.io": "^4.5.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "4.17.13",
    "@types/hapi__joi": "^17.1.15",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "18.7.1",
    "@types/node-cron": "^3.0.6",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nodemon": "^2.0.21",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.8.2"
  }
}
